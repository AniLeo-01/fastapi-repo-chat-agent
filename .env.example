# FastAPI Repo Chat Agent - Environment Variables
# Copy this file to .env and fill in your values

# Required: OpenAI API Key for LLM functionality
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: LLM Model (default: gpt-4o-mini)
LLM_MODEL_ID=gpt-4o-mini

# Optional: Repository to index (default: FastAPI)
FASTAPI_REPO_URL=https://github.com/fastapi/fastapi.git

# Neo4j Configuration (defaults work with docker-compose)
# For local development without Docker:
# NEO4J_URI=bolt://localhost:7687
# For Docker:
# NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

